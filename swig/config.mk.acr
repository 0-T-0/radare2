VERSION=@VERSION@
RELEASE=1

CC=@USERCC@
CXX=@USERCXX@
OSTYPE=@USEROSTYPE@
PREFIX=@PREFIX@

ifeq ($(OSTYPE),windows)
SOEXT=dll
else
ifeq ($(OSTYPE),darwin)
SOEXT=dylib
else
# linux, bsd, ...
SOEXT=so
endif
endif

INSTALL_DIR=@INSTALL_DIR@
INSTALL_DATA=@INSTALL_DATA@
INSTALL_PROGRAM=@INSTALL_PROGRAM@
INSTALL_MAN=@INSTALL_MAN@

USE_VALASWIG=@USE_VALASWIG@
HAVE_VALASWIG=@HAVE_VALASWIG@
HAVE_SWIG=@HAVE_SWIG@

ifneq ($(HAVE_SWIG)$(HAVE_VALASWIG)$(USE_VALASWIG),111)
HAVE_VALASWIG=0
endif
